@page
@model CreateModel

@{
    ViewData["Title"] = "Add Lab";
}

<h1>Add Lab</h1>

<h4>Lab Information</h4>
<hr />
<div class="row">
    <form class="row g-3" method="post">
        <partial name="~/Areas/Admin/Pages/Shared/Labs/AddEdit.cshtml" model="@Model.Lab" />
            
        <div class="row g-3">
            <div class="col-md-2">
                <input type="submit" value="Create" class="w-auto btn btn-primary" />
            </div>
        </div>
    </form>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
